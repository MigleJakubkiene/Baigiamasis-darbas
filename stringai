# Pirma užduotis
print("Pirma užduotis")
text="Migle Jakubkiene"
print(text)
print(len(text))

# Atra užduotis
print("Antra užduotis")
tekstas="Sukūrė įrankį, leidžiantį pasikalbėti su savimi iš ateities ir pamatyti, koks būsite senatvėje: eksperimento rezultatai intriguoja"
print(tekstas)
print(len(tekstas))

# Trečia užduotis
print("Trečia užduotis")
print(text.upper())
print(text.lower())

# Ketvirta užduotis
print("Ketvirta užduotis")
print(tekstas[0:9])
print(tekstas[-10:-1],tekstas[len(tekstas)-1])
print(tekstas[len(tekstas)-10:])
print(tekstas[10:20])
print(tekstas[::2])

#Penkta užduotis
print("Penkta užduotis")
print(tekstas.count("a"))
print(tekstas.find("a"))

#Šešta užduotis
print("Šešta užduotis")
txt="Labas rytas"
print(txt)
print(txt[1:len(txt)])
print(txt[0:len(txt)-1])
print(txt[1:len(txt)-1])

#Papildoma pirma
print("Papildoma pirma")
sakinys="An American in Paris"
print(sakinys.replace('A','**').replace('a','**'))

#Papildoma antra
print("Papildoma antra")
print(sakinys.count("A"))

#Papildoma trečia
print("Papildoma trečia")
print(sakinys.replace("A","").replace("a","").replace("e","").replace("i",""))
